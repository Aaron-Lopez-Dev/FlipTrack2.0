generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model vehicles {
  id      Int @id @default(autoincrement())
  year    Int
  make    String
  model   String
  mileage Int
  price   Int
}

model items {
  id      Int @id @default(autoincrement())
  name    String
  price   Int
}